{
	"Forms": {
		"firstName": { "regexp": "[A-Z][a-z]" },
		"lastName": { "regexp": "[A-Z][a-z]" },
		"avatarUrl": {
			"regexp": "https://i\\.pravatar\\.cc/150\\?u=[0-9]{5}"
		},
		"statusMessage": {
			"string": ["working from home", "watching Netflix"]
		},
		"email": {
			"regexp": "#[a-z]{5,10}@[a-z]{5}\\.[a-z]{2,3}"
		},
		"color": {
			"regexp": "#[0-9A-F]{6}"
		},
		"transactionAmount": { "int": { "min": 500, "max": 10000 } },
		"commission": { "int": { "min": 10, "max": 100 } },
		"createdAt": {
			"dateTime": {
				"min": "2022-01-01T00:00:00Z",
				"max": "2022-12-31T23:59:59Z"
			}
		}
	}
}
